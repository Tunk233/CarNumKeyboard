<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 自定义车牌输入框-->
    <declare-styleable name="CarNumView">
        <!--输入框的宽度-->
        <attr name="box_width" format="dimension|reference" />
        <!--输入框的高度-->
        <attr name="box_height" format="dimension|reference" />
        <!--输入框之间的分割线-->
        <attr name="box_divider_drawable" format="reference" />
        <!--输入框文字颜色-->
        <attr name="box_text_color" format="color|reference" />
        <!--输入框文字大小-->
        <attr name="box_text_size" format="dimension|reference" />
        <!--输入框获取焦点时边框-->
        <attr name="box_bg_focus" format="reference" />
        <!--输入框没有焦点时边框-->
        <attr name="box_bg_normal" format="reference" />
    </declare-styleable>
</resources>